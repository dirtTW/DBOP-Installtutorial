            modrinthlink="https://modrinth.com/modpack/tnvbp-lite"
            curseforgelink=""
            version="Fabric 1.18.2 - 1.21"
          />
          <Card
            name="Simply Optimized"
            imagePath="https://cdn.modrinth.com/data/BYfVnHa7/7f8dc20fc0edd29fd95819a6f40938be0b9cadfa.png"
            altText="領先的、經過深入研究的優化模組包，專注於純粹的性能。"
            modrinthlink="https://modrinth.com/modpack/sop"
            curseforgelink=""
            version="Fabric 1.16.5 - 1.21.1"
          />
          <Card
            name="All of Fabric 6 - AOF6"
            imagePath="https://media.forgecdn.net/avatars/630/292/638029267991555056.png"
            altText="All of Fabric 6 是 AOF 系列的第六作，保持了前作的許多特點, 有許多任務 以及新戰鬥系統 跟各種建築可以探索。"
            modrinthlink=""
            curseforgelink="https://www.curseforge.com/minecraft/modpacks/all-of-fabric-6"
            version="Fabric 1.19.2"
          />
          <Card
            name="All of Fabric 7 - AOF7"
            imagePath="https://media.forgecdn.net/avatars/861/617/638269386500250203.png"
            altText="All of Fabric 7 是 AOF 系列的第七作，足跡與故事 探險開始!!!!"
            modrinthlink=""
            curseforgelink="https://www.curseforge.com/minecraft/modpacks/all-of-fabric-7"
            version="Fabric 1.20.1"
          />
