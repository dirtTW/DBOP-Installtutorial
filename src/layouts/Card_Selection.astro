---
import Card from "../components/Card.astro";
---

<main>
  <div class="p-4">
    <div
      class="p-4 border-2 border-gray-200 rounded-lg dark:border-gray-700 mt-14"
    >
      <div class="flex">
        <div class="mb-13 grow">
          <Card
            name="DBOP (DirtTW's Basement Optimize Pack) (土靈的地下室優化包)"
            imagePath="https://cdn.modrinth.com/data/olssWAmw/2a3d3855b9926df7874a94a96bed3cc0ff5e1f35.png"
            altText="DBOP 是一個優化模組包 大部分模組都是用戶端並且能夠優化 Minecraft 麥塊效能。"
            modrinthlink="https://modrinth.com/modpack/dbop"
            curseforgelink="https://www.curseforge.com/minecraft/modpacks/dbop"
            version="Fabric 1.19.4 - 1.21.1"
          />
          <Card
            name="DUP (DirtTW's Utility Pack) (土靈的實用包)"
            imagePath="https://cdn.modrinth.com/data/fT7MQtrV/51c5ccae57a8062b8b6d0c5bb244373b15a8e948.png"
            altText="DUP 是一個補助類的實用模組包裡面包含藍圖模組，小地圖模組以及整理背包模組等等的。"
            modrinthlink="https://modrinth.com/modpack/dup"
            curseforgelink=""
            version="Fabric 1.20.1 - 1.21"
          />
          <Card
            name="Fabulously Optimized"
            imagePath="https://cdn.modrinth.com/data/1KVo5zza/d8152911f8fd5d7e9a8c499fe89045af81fe816e.png"
            altText="使用這個簡單的模組包來提高您的圖形和效能。 1.21"
            modrinthlink="https://modrinth.com/modpack/fabulously-optimized"
            curseforgelink="https://www.curseforge.com/minecraft/modpacks/fabulously-optimized"
            version="Fabric 1.16.5 - 1.21.1"
          />
          <Card
            name="TNVBP"
            imagePath="https://cdn.modrinth.com/data/Ohwb2cTF/cde595ad9e8d3a6e2b483f164d79180cceec2d53.png"
            altText="Minecraft 效能優化模組包丨TechNoob's Vanilla Boost Pack"
            modrinthlink="https://modrinth.com/modpack/tnvbp"
            curseforgelink=""
            version="Fabric 1.18.2 - 1.21"
          />
          <Card
            name="TNVBP Lite"
            imagePath="https://cdn.modrinth.com/data/Er2bMzL7/a14629621f005d8b4bd7101208e1e30c87610a07.png"
